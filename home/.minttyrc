# See `man mintty'.

# Looks
ForegroundColour=108,131,134
BackgroundColour=0,24,40
CursorColour=0,255,0
Transparency=off
OpaqueWhenFocused=yes
CursorType=underscore

# Text
Font=Powerline Consolas
FontHeight=8
BoldAsFont=yes
BoldAsColour=yes
AllowBlinking=yes
Locale=C
Charset=UTF-8

# Keys
SwitchShortcuts=no

# Mouse
RightClickAction=paste

# Window
Columns=132
Rows=50
ScrollbackLines=100000

# Terminal
Term=xterm-256color
BellSound=yes
BellFlash=yes
