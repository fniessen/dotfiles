#!/bin/sh
workingDir='d:/Users/fni/src/emacs-leuven/'
TESTEL=
TESTFILE=~/test.el
if test -e $TESTFILE # file exists
then
    TESTEL="-l $TESTFILE"
fi

EMACS="emacs"

# No init file.
cmd="$EMACS -q -l ${workingDir}.emacs-minimal.el $TESTEL"

echo "Launching '$cmd'..."
eval "$cmd"

echo "Exit status: $?"
